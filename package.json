{
  "name": "@totonoo/vue-codemirror",
  "version": "1.0.7",
  "main": "./dist/vue-codemirror.umd.js",
  "module": "./dist/vue-codemirror.es.js",
  "typings": "./dist_types/main.d.ts",
  "files": [
    "dist/*",
    "dist_types/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-codemirror.es.js",
      "require": "./dist/vue-codemirror.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build_o": "vue-tsc --noEmit && vite build",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "test": ""
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.19.1",
    "@codemirror/lang-css": "^0.19.3",
    "@codemirror/lang-html": "^0.19.4",
    "@codemirror/lang-javascript": "^0.19.7",
    "@codemirror/lang-json": "^0.19.2",
    "@codemirror/lang-markdown": "^0.19.6",
    "@lezer/markdown": "^0.15.5",
    "markdown-it": "^12.3.2",
    "prettier": "^2.5.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/svgo": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^2.2.0",
    "sass": "^1.49.8",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-svg-loader": "^3.1.2",
    "vue-tsc": "^0.29.8"
  },
  "description": "codemirror vue组件",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myestorm/vue-codemirror.git"
  },
  "keywords": [
    "vue3",
    "codemirror next",
    "typescript"
  ],
  "author": "totonoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/myestorm/vue-codemirror/issues"
  },
  "homepage": "https://github.com/myestorm/vue-codemirror#readme"
}
